    <!-- Center Info -->
    <string name="center_info_title">Centrum</string>
    <string name="center_info_organization">Sociedad Protectora de Animales de Malaga</string>
    <string name="center_info_about_title">O Nas</string>
    <string name="center_info_about_description">Sociedad Protectora de Animales de Malaga to organizacja non-profit poswiecona ratowaniu, rehabilitacji i znajdowaniu nowych domow dla porzuconych i maltretowanych zwierzat w prowincji Malaga. Zalozeni z misja dania kazdemu zwierzeciu drugiej szansy, niestrudzenie pracujemy, aby zapewnic, ze psy i koty pod nasza opieka otrzymaja milosc, opieke medyczna i schronienie, na ktore zasluguja, czekajac na swoje ostateczne rodziny.</string>
    <string name="center_info_mission_title">Nasza Misja</string>
    <string name="center_info_mission_description">Wierzymy, ze kazde zwierze zasluguje na zycie wolne od cierpienia. Nasza misja to ratowanie zwierzat w potrzebie, zapewnianie im opieki weterynaryjnej i rehabilitacji, promowanie odpowiedzialnego posiadania zwierzat poprzez edukacje oraz znajdowanie kochajacych, stalych domow dla kazdego zwierzecia, ktore przechodzi przez nasze drzwi. Blisko wspolpracujemy takze z lokalnymi wladzami i spolecznosciami, aby zmniejszyc porzucanie zwierzat.</string>
    <string name="center_info_services_title">Co Robimy</string>
    <string name="center_info_service_rescue">Ratowanie i dawanie schronienia porzuconym, zagubionym i maltretowanym zwierzetom</string>
    <string name="center_info_service_veterinary">Zapewnianie kompleksowej opieki weterynaryjnej wlaczajac szczepienia, sterylizacje i leczenie</string>
    <string name="center_info_service_adoption">Zarzadzanie odpowiedzialnymi procesami adopcji, aby laczyc zwierzeta z kochajacymi rodzinami</string>
    <string name="center_info_service_education">Prowadzenie programow edukacyjnych na temat dobrostanu zwierzat i odpowiedzialnego posiadania</string>
    <string name="center_info_service_sterilization">Promowanie kampanii sterylizacji w celu kontroli populacji bezdomnych zwierzat</string>
    <string name="center_info_visiting_title">Godziny Odwiedzin</string>
    <string name="center_info_visiting_weekdays">Poniedzialek do piatku: 10:00 - 14:00 i 16:00 - 18:00</string>
    <string name="center_info_visiting_weekends">Soboty, niedziele i swieta: 10:00 - 14:00</string>
    <string name="center_info_visiting_note">Zalecamy wczesniejszy kontakt telefoniczny w celu umowienia wizyty i zapewnienia najlepszej obslugi.</string>
    <string name="center_info_location_title">Lokalizacja</string>
    <string name="center_info_address">Camino de las Erizas, 4, 29011 Malaga</string>
    <string name="center_info_phone">951 946 977</string>
    <string name="center_info_email">contacto@protectoramalaga.com</string>
    <string name="center_info_social_title">Obserwuj Nas</string>
    <string name="center_info_social_description">Badz z nami w kontakcie w mediach spolecznosciowych, aby zobaczyc nasze najnowsze akcje ratunkowe, historie udanych adopcji i nadchodzace wydarzenia.</string>
    <string name="center_info_help_title">Jak Mozesz Pomoc</string>
