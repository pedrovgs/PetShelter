    <string name="center_info_social_title">Volg Ons</string>
    <string name="center_info_social_description">Blijf verbonden met ons op sociale media om onze laatste reddingen, adoptiesuccessen en aankomende evenementen te zien.</string>
    <string name="center_info_help_title">Hoe U Kunt Helpen</string>
