    <string name="center_info_social_title">Folgen Sie Uns</string>
    <string name="center_info_social_description">Bleiben Sie mit uns in den sozialen Medien verbunden, um unsere neuesten Rettungen, Adoptionserfolge und bevorstehende Veranstaltungen zu sehen.</string>
    <string name="center_info_help_title">Wie Sie Helfen Konnen</string>
